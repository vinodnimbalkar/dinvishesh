{
  "name": "dinvishesh",
  "version": "0.1.3",
  "description": "A simple NPM Package which returns dinvishesh (On This Day in History). Get your daily dinvishesh and increase knowledge!",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": ["dist", "src"],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "coverage": "tsdx test --coverage"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinodnimbalkar/dinvishesh.git"
  },
  "bugs": {
    "url": "https://github.com/vinodnimbalkar/dinvishesh/issues"
  },
  "homepage": "https://github.com/vinodnimbalkar/dinvishesh#readme",
  "keywords": [
    "dinvishesh",
    "marathi",
    "जन्म",
    "घटना",
    "मृत्यू"
  ],
  "author": "Vinod Nimbalkar",
  "module": "dist/dinvishesh.esm.js",
  "size-limit": [{
    "path": "dist/dinvishesh.cjs.production.min.js",
    "limit": "3 MB"
  }, {
    "path": "dist/dinvishesh.esm.js",
    "limit": "3 MB"
  }],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9"
  }
}